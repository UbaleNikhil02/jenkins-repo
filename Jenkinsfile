    agent any

    stages {
        stage ('Compile Stage') {

            steps {
                
                    sleep 10
                }
            
        }

        stage ('Testing Stage') {

            steps {
                
                    sleep 20
                }
            
        }


        stage ('Install Stage') {
            steps {
                
                    sleep 30
                }
            
        }
        
